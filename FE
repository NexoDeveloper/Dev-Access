local function LUQYATO_fake_script() -- Main.HomeOpen 
	local script = Instance.new('LocalScript', Main)

	script.Parent.SideBar.Home1.MouseButton1Click:Connect(function()
		script.Parent.Home.Visible = true
		script.Parent.A1.Visible = false
		script.Parent.A2.Visible = false
	end)
end
coroutine.wrap(LUQYATO_fake_script)()
local function NERAL_fake_script() -- Main.SendNotification 
	local script = Instance.new('LocalScript', Main)

	game.StarterGui:SetCore("SendNotification",{
		Title = "Nexo's FE"; 
		Text = "Enjoy :)";
		Icon = "";
		Duration = 5;
	})
	
end
coroutine.wrap(NERAL_fake_script)()
