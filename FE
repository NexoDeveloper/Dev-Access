-- Scripts:

local function QLNWCS_fake_script() -- R1.LocalScript 
	local script = Instance.new('LocalScript', R1)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.PlayerAdded:Connect(function(plr) -- Making the players name plr
			plr.CharacterAdded:Connect(function(chr) -- Making the characters name chr
				repeat wait()until chr.Humanoid
				chr.Humanoid.Died:Connect(function() -- Making sure that this happens only when death
					plr:LoadCharacter() -- Loading the character
				end)
			end)
		end)
	end)
end
coroutine.wrap(QLNWCS_fake_script)()
local function MIRJNSH_fake_script() -- Main.ToggleDragGui 
	local script = Instance.new('LocalScript', Main)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(MIRJNSH_fake_script)()
local function RGXQ_fake_script() -- Main.MainOpen 
	local script = Instance.new('LocalScript', Main)

	script.Parent.SideBar.N1.MouseButton1Click:Connect(function()
		script.Parent.Home.Visible = false
		script.Parent.A1.Visible = true
		script.Parent.A2.Visible = false
	end)
end
coroutine.wrap(RGXQ_fake_script)()
local function WOQOKXG_fake_script() -- k2.LocalScript 
	local script = Instance.new('LocalScript', k2)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/MonkoTubeYT/animationedit/main/GUI',true))()
	end)
end
coroutine.wrap(WOQOKXG_fake_script)()
